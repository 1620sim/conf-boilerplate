<!-- NAVIGATION -->
<nav id="nav">
  <ul class="wrapper">
    <% for name, enabled of @sections : %>
      <% if enabled : %>
        <li class="nav-item">
          <a href="#<%= name %>" title="<%= @labels[name] %>" class="nav-link"><%= @labels[name] %></a>
        </li>
      <% end %>
    <% end %>
  </ul>
</nav>

<hr />
